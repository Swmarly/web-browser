<!--
This file is used for testing only, see dwa.xml for configuration used in
production.
-->

<dwa-configuration>

<event name="DwaTestMetric">
  <owner>user@example.com</owner>
  <summary>
    A test DWA metric.
  </summary>
  <study name="ACTStudy"/>
  <study name="LegacyStudy" expires_after="M135"/>
  <study name="ModeBStudy"/>
  <metric name="HasVideo" enum="Boolean">
    <summary>
      True if the page contains a video.
    </summary>
  </metric>
  <metric name="Latency" expires_after="2025-07-25">
    <summary>
      The latency in ms.
    </summary>
  </metric>
</event>

</dwa-configuration>
