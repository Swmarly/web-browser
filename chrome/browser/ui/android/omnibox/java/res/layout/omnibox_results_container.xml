<org.chromium.chrome.browser.omnibox.suggestions.OmniboxSuggestionsContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/omnibox_results_container"
    android:background="@android:color/transparent"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:importantForAccessibility="yes"
    android:descendantFocusability="blocksDescendants">

    <org.chromium.chrome.browser.omnibox.suggestions.OmniboxSuggestionsDropdown
        android:id="@+id/omnibox_suggestions_dropdown"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"/>

</org.chromium.chrome.browser.omnibox.suggestions.OmniboxSuggestionsContainer>
