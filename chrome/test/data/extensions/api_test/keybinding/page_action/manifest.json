{
  "name": "A page action that sends a 'clicked' test message",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "http://*/*"
  ],
  "action": {
    "default_title": "Send message",
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": "Alt+Shift+F",
      "description": ""
    }
  }
}
