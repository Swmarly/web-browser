{
  "manifest_version": 3,
  "name": "Kiosk Input API Test Extension",
  "description": "Companion Extension for KioskEnterpriseInputApiBrowserTest",
  "version": "1.0",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png"
  },
  "permissions": [
    "enterprise.kioskInput"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  }
}
